<SfStockChart @ref="chart" Title="@title">
    <StockChartCrosshairSettings Enable="true" LineType="LineType.Vertical"></StockChartCrosshairSettings>
    <StockChartTooltipSettings Enable="true" Shared="true"></StockChartTooltipSettings>
    <StockChartSeriesCollection>
        <StockChartSeries DataSource="@OuterStockDetails" Type="ChartSeriesType.Candle" XName="T" YName="C" High="H" Low="L" Open="O" Close="C" Volume="V" />
    </StockChartSeriesCollection>
</SfStockChart>

@code {
    SfStockChart chart;
    string title = "OHCL";

    [Parameter] public List<SingleOHLCDate> OuterStockDetails { get; set; }
}